{
  "name": "map-platform",
  "version": "1.0.0",
  "description": "Платформа для отображения потребности",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run minify",
    "minify": "npm run minify:js && npm run minify:css",
    "minify:js": "terser public/script.js -o public/script.min.js -c -m",
    "minify:css": "cleancss -o public/styles.min.css public/styles.css"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "terser": "^5.19.2",
    "clean-css-cli": "^5.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": ["map", "platform", "yandex", "google-sheets"],
  "author": "Your Name",
  "license": "MIT"
}